{
    "name": "restin.ai",
    "version": "17.0.0",
    "private": true,
    "packageManager": "npm@10.2.4",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "clean": "turbo run clean && rm -rf node_modules"
    },
    "devDependencies": {
        "turbo": "latest",
        "prettier": "^3.2.5",
        "typescript": "^5.4.5"
    },
    "engines": {
        "node": ">=18"
    }
}